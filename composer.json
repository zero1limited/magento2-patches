{
    "name": "zero1limited/magento2-patches",
    "description": "Patches for core Magento2",
    "authors": [
        {
            "name": "Arron Moss",
            "email": "arron.moss@zero1.co.uk"
        }
    ],
    "keywords": [
        "magento2 patches",
        "magento2",
        "patching",
        "patches"
    ],
    "type": "magento2-module",
    "version": "1.0.19",
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Zero1\\Patches\\": ""
        }
    },
    "require": {
        "composer/semver": "^1.0",
        "cweagans/composer-patches": "^1.6.5"
    },
    "suggest": {
        "mdoq/module-connector": "Allows you to apply and test patches quickly and safely."
    }
}
